<mvc:View
    controllerName="uimodule.controller.AdminPanel"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    height="100%">
    <VBox renderType="Bare" alignItems="Center" height="100%" class="adminPanelContainer">
        <tnt:SideNavigation id="sideNavigation" expanded="false" class="adminPanelToggle" itemSelect=".onItemSelect">
            <tnt:NavigationList >
                <tnt:NavigationListItem text="Users" icon="sap-icon://person-placeholder" key="Users" />
                <tnt:NavigationListItem text="Products" icon="sap-icon://product" key="Products" />
                <tnt:NavigationListItem text="Add Product" icon="sap-icon://add-product" key="ProductsAdd" />
            </tnt:NavigationList>
        </tnt:SideNavigation>
    </VBox>
</mvc:View>
